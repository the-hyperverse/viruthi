{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Main",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/main.js",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "npm: build"
    },
    {
      "name": "Renderer",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/dist",
      "preLaunchTask": "npm: build"
    }
  ]
}
